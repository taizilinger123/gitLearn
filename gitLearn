$ cat  .gitconfig
[http]
        sslVerify = false
[user]
        email = 837337164@qq.com
        name = taizlinger123
[filter "lfs"]
        clean = git-lfs clean -- %f
        smudge = git-lfs smudge -- %f
        process = git-lfs filter-process
        required = true
[credential]
helper = store --file .git-credentials
#重新再打开git bash第一次要输入用户名和密码以后再也不用输入了
